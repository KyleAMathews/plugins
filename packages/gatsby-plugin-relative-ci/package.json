{
  "name": "gatsby-plugin-relative-ci",
  "version": "1.0.0",
  "description": "Integrate your Gatsby site with Relative CI",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "relative",
    "ci"
  ],
  "main": "index.js",
  "repository": "git@github.com:moonmeister/gatsby-source-packagist.git",
  "author": "Alex Moon <alex.jared.moon@gmail.com>",
  "license": "MIT",
  "dependencies": {
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "format": "prettier --write .",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "babel-preset-gatsby-package": "^1.1.0",
    "cross-env": "^7.0.2",
    "prettier": "^2.0.4"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0"
  }
}
